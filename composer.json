{
    "name": "dpagini/davidpagini",
    "description": "Website of David Henry Pagini",
    "type": "project",
    "authors": [
        {
            "name": "Dave Pagini",
            "email": "davidpagini@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=8.1",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal-tome/tome_drush": "dev-master",
        "drupal/admin_toolbar": "^2.4",
        "drupal/coder": "^8.3",
        "drupal/core-composer-scaffold": "^9.0.0",
        "drupal/core-recommended": "^9.0.0",
        "drupal/google_analytics": "^4.0",
        "drupal/gutenberg": "^2.5",
        "drupal/honeypot": "^2.0",
        "drupal/metatag": "^1.14",
        "drupal/pathauto": "^1.8",
        "drupal/tome": "^1.8",
        "drupal/webform": "^6",
        "drush/drush": "^11.3"
    },
    "require-dev": {
        "drupal/devel": "^4.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "acquia/blt": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "patches": {
            "drupal/gutenberg": {
                "Welcome to Gutenberg editor popup appears for anonymous user": "https://www.drupal.org/files/issues/2022-10-05/3310913-9.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    }
}
